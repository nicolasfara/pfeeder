{
    "name": "backend",
    "version": "0.1.0",
    "description": "Pfeeder backend API",
    "repository": {
        "type": "git",
        "url": ""
    },
    "author": "Nicolas Farabegoli",
    "license": "MIT",
    "scripts": {
        "start": "npm run serve",
        "build": "npm run build-sass && npm run build-ts && npm run lint && npm run copy-static-assets",
        "serve": "node dist/server.js",
        "watch-node": "nodemon dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
        "test": "jest --forceExit --coverage --verbose",
        "watch-test": "npm run test -- --watchAll",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
        "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "copy-static-assets": "ts-node copyStaticAssets.ts",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
    },
    "dependencies": {
        "@types/multer": "^1.4.3",
        "@types/passport-jwt": "^3.0.3",
        "async": "^3.2.0",
        "bcryptjs": "^2.4.3",
        "bluebird": "^3.7.2",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "connect-mongo": "^3.2.0",
        "dotenv": "^8.2.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-openapi-validator": "^3.16.4",
        "express-session": "^1.17.1",
        "express-validator": "^6.5.0",
        "fbgraph": "^1.4.4",
        "lodash": "^4.17.15",
        "lusca": "^1.6.1",
        "mongoose": "^5.9.15",
        "nodemailer": "^6.4.6",
        "passport": "^0.4.1",
        "passport-facebook": "^3.0.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "passport-unique-token": "^1.0.0",
        "pug": "^2.0.4",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/async": "^3.2.3",
        "@types/bcryptjs": "^2.4.2",
        "@types/bluebird": "^3.5.32",
        "@types/body-parser": "^1.19.0",
        "@types/chai": "^4.2.11",
        "@types/compression": "^1.7.0",
        "@types/concurrently": "^5.2.0",
        "@types/connect-mongo": "^3.1.3",
        "@types/errorhandler": "^1.5.0",
        "@types/eslint": "^6.8.1",
        "@types/express": "^4.17.6",
        "@types/express-flash": "0.0.2",
        "@types/express-session": "^1.17.0",
        "@types/jest": "^25.2.3",
        "@types/jquery": "^3.3.38",
        "@types/lodash": "^4.14.152",
        "@types/lusca": "^1.6.1",
        "@types/mongoose": "^5.7.21",
        "@types/node": "^14.0.4",
        "@types/node-sass": "^4.11.1",
        "@types/nodemailer": "^6.4.0",
        "@types/passport": "^1.0.3",
        "@types/passport-facebook": "^2.1.10",
        "@types/passport-local": "^1.0.33",
        "@types/passport-unique-token": "^1.0.2",
        "@types/pug": "^2.0.4",
        "@types/request": "^2.48.5",
        "@types/request-promise": "^4.1.46",
        "@types/shelljs": "^0.8.8",
        "@types/supertest": "^2.0.9",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
